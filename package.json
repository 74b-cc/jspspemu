{
  "scripts": {
    "build": "npx webpack --mode production --config=webpack.prod.js",
    "serve": "npx webpack serve",
    "test": "npx tsc && npx -n --enable-source-maps mocha output/test/_tests.js",
    "integration_test": "npx tsc && npx -n --enable-source-maps mocha output/test/_itests.js",
    "docgen": "npx tsc && node output/src/docgen.js",
    "benchmark": "npx tsc && node --enable-source-maps output/src/headless.js data/benchmark/benchmark.prx",
    "benchmark_interpreted": "npx tsc && node --enable-source-maps output/src/headless.js -interpreted data/benchmark/benchmark.prx",
    "headless": "npx tsc && node --enable-source-maps output/src/headless.js"
  },
